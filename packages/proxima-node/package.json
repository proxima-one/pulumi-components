{
  "name": "@proxima-one/pulumi-proxima-node",
  "version": "0.14.25",
  "description": "pulumi components for proxima",
  "repository": "git@github.com:proxima-one/pulumi-components.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf -rf dist && rimraf tsconfig.tsbuildinfo",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "tsc --build && yarn copy-content-files",
    "test": "jest --passWithNoTests",
    "copy-content-files": "yarn copyfiles -u 1 \"src/**/*\" dist/",
    "coverage": "jest --coverage",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:app": "yarn start app start --",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch",
    "gen:jsonschema:mongodb": "json2ts -i ./src/components/mongodb/values.schema.json -o ./src/components/mongodb/values.ts",
    "gen:jsonschema:postgresql": "json2ts -i ./src/components/postgresql/values.schema.json -o ./src/components/postgresql/values.ts"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.16.1",
    "@proxima-one/proxima-config": "0.3.1-preview.eb64fb9.0+eb64fb9",
    "@proxima-one/proxima-utils": "^0.1.5",
    "@proxima-one/pulumi-k8s-base": "^0.1.6",
    "@pulumi/kubernetes": "^3.0.0",
    "@pulumi/kubernetesx": "^0.1.5",
    "@pulumi/minio": "^0.2.0",
    "@pulumi/pulumi": "^3.0.0",
    "@pulumi/random": "^4.3.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copyfiles": "^2.4.1",
    "jest": "^27.4.5",
    "json-schema-to-typescript": "^10.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "preset": "<rootDir>/"
  },
  "gitHead": "2412b8ad89fbf18ee838367b774b07b21a62900c"
}
